{
   "name": "dddfile-data",
   "version": "0.0.1",
   "description": "",
   "type": "module",
   "devDependencies": {
      "@types/node": "^18.11.18",
      "cross-env": "^7.0.3",
      "cross-var": "^1.1.0",
      "ts-node": "10.9.1",
      "typescript": "4.9.4"
   },
   "dependencies": {
      "dotenv": "^16.0.3",
      "pg": "^8.8.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.11",
      "typeorm-naming-strategies": "^4.1.0"
   },
   "scripts": {
      "start": "ts-node --esm index.ts",
      "build": "tsc",
      "typeorm": "typeorm-ts-node-esm",
      "test": "cross-var npm run typeorm $npm_config_migration_name",
      "migration:create": "cross-var npx typeorm-ts-node-esm migration:create src/migration/$npm_config_name",
      "migration:generate": "cross-var npx typeorm-ts-node-esm migration:generate -d src/data-source.ts src/migration/$npm_config_name",
      "migration:run": "npx typeorm-ts-node-esm migration:run -d src/data-source.ts",
      "migration:run:prod": "cross-env NODE_ENV=production npx typeorm-ts-node-esm migration:run -d src/data-source.ts",
      "migration:revert": "npm run typeorm -- migration:revert"
   }
}
