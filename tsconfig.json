{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "ES2022",
      "module": "ES2022",
      "moduleResolution": "nodenext",
      "esModuleInterop": true,
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "declarationMap": true
   }
}